<h1>Committees</h1>

<table>
  <thead>
    <tr>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @committees.each do |committee| %>
      <tr>
        <td><%= committee.name %></td>
        <td><%= link_to 'Show', admin_committee_path(committee) %></td>
        <td><%= link_to 'Edit', edit_admin_committee_path(committee) %></td>
        <td><%= link_to 'Destroy', admin_committee_path(committee), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Committee', new_admin_committee_path %>
