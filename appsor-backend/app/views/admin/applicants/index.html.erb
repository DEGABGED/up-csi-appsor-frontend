<p id="notice"><%= notice %></p>

<h1>Applicants</h1>

<table>
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Student Number</th>
      <th>Email</th>
      <th>Contact Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @applicants.each do |applicant| %>
      <tr>
        <td><%= applicant.name %></td>
        <td><%= applicant.student_number %></td>
        <td><%= applicant.email %></td>
        <td><%= applicant.contact_number %></td>
        <td><%= link_to 'Show', admin_applicant_path(applicant) %></td>
        <td><%= link_to 'Edit', edit_admin_applicant_path(applicant) %></td>
        <td><%= link_to 'Destroy', admin_applicant_path(applicant), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Applicant', new_admin_applicant_path %>
